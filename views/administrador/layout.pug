    header(class="position-fixed z-2" style="top: 0; background-color: #f0f0f0;width:100%")
      block header
        include navTop
    
    main(class="d-flex  position-relative" )
    
      block aside
      div(id="divContent" class="content col-12 col-md-6 col-lg-4 z-1" style="margin-top:3rem;margin-left: 200px;padding: 20px;width: calc(100% - 200px);" )
        block content
         if rta
          .alert(class=`alert-${rta.alertType}`) 
           p= rta.alertMessage
        
    footer
      block footer
        //- Bloque por defecto para el footer
        p &copy; 2024 Mi sitio web