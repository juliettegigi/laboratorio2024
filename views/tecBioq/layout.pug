doctype html
html
  head
    title= title 
    //- title lo envía el server o no {title:"mi title"}
    block head
      //- Bloque por defecto para el head. Si no es sobrescrito, este contenido permanecerá.
      meta(charset='UTF-8')
      meta(name="viewport" content="width=device-width, initial-scale=1.0")
      link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous")
     
      script(src="https://code.jquery.com/jquery-3.7.0.js")
      link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css")
      script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous")
      script(src="/socket.io/socket.io.js")
      script(src="/javascripts/socket-client.js")
    block styles  
      link(rel='stylesheet', href='/stylesheets/tecbioq/body.css')
      link(rel='stylesheet', href='/stylesheets/tecbioq/style.css')
      link(rel='stylesheet', href='/stylesheets/tecbioq/addCategDet.css')
      link(rel='stylesheet', href='/stylesheets/tecbioq/formExamen.css')
      link(rel='stylesheet', href='/stylesheets/tecbioq/menuLateralExamen.css')
      link(rel='stylesheet', href='/stylesheets/tecbioq/inputSearch.css')
      link(rel='stylesheet', href='/stylesheets/tecbioq/examenDatos.css')
      link(rel='stylesheet', href='/stylesheets/tecbioq/divContent.css')
      link(rel='stylesheet', href='/stylesheets/tecbioq/formValorRef.css')
  body 
    header( style="top: 0; background-color: #f0f0f0;width:100%")
      block header
        include navTop
    
    main(class="d-flex  position-relative" )
      block aside
      block content
       div(id="divContentLista" )
         include ./inputSearch.pug
         +inputSearch("/tecBioq","nombre/codigo a buscar","Crear Examen")
         include ./listaExamenes.pug
        
    footer
      block footer
        //- Bloque por defecto para el footer
        p &copy; 2024 Mi sitio web